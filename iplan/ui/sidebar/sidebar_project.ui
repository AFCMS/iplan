<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="SidebarProject" parent="GtkListBoxRow">
    <child>
      <object class="GtkLabel" id="content">
        <property name="halign">start</property>
        <property name="use-markup">true</property>
      </object>
    </child>
    <child><!-- Drag Source -->
      <object class="GtkDragSource">
        <property name="actions">move</property>
        <property name="propagation-phase">capture</property>
        <signal name="prepare" handler="on_drag_prepare" swapped="no"/>
        <signal name="drag-begin" handler="on_drag_begin" swapped="no"/>
        <signal name="drag-cancel" handler="on_drag_cancel"/>
      </object>
    </child><!-- End Drag Source -->
  </template>
</interface>
